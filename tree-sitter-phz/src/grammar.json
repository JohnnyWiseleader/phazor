{
  "name": "phz",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "statement"
      }
    },
    "statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@route"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "STRING",
          "value": "def"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "STRING",
          "value": ")"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "SYMBOL",
          "name": "html_block"
        }
      ]
    },
    "string": {
      "type": "PATTERN",
      "value": "\"[^\"]*\""
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z_]\\w*"
    },
    "html_block": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "html_element"
      }
    },
    "html_element": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "open_tag"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "inner_html"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "close_tag"
        }
      ]
    },
    "open_tag": {
      "type": "PATTERN",
      "value": "<[a-zA-Z][^>]*>"
    },
    "close_tag": {
      "type": "PATTERN",
      "value": "<\\/[a-zA-Z][^>]*>"
    },
    "inner_html": {
      "type": "PATTERN",
      "value": "[^<]+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

